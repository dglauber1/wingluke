{
  "root": true,
  "project": {
    "name": "Wing Luke Museum"
  },
  "api": {
    "projectId": "1jr4bwbd",
    "dataset": "production"
  },
  "env": {
    "development": {
      "api": {
        "dataset": "development"
      },
      "plugins": [
        "@sanity/vision",
        "@sanity/dashboard",
        "dashboard-widget-cats"
      ]
    }
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool"
  ],
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "implements": "part:@sanity/dashboard/config",
      "path": "./custom_config/dashboardConfig.js"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./custom_config/deskStructure.js"
    },
    {
      "name": "part:@sanity/base/new-document-structure",
      "path": "./custom_config/newDocumentStructure.js"
    },
    {
      "implements": "part:@sanity/base/document-actions/resolver",
      "path": "./custom_config/resolveDocumentActions.js"
    }
  ]
}
